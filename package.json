{
    "private": true,
    "name": "blog",
    "description": "Preact's netlify CMS template",
    "version": "0.1.3",
    "license": "MIT",
    "scripts": {
        "start": "per-env",
        "start:production": "npm run -s serve",
        "start:development": "npm run -s dev",
        "build": "preact build --prerenderUrls ./prerender-urls.js",
        "dev": "preact watch --prerender --prerenderUrls ./prerender-urls.js",
        "lint": "eslint src",
        "test": "jest"
    },
    "eslintConfig": {
        "extends": "eslint-config-synacor"
    },
    "eslintIgnore": [
        "build/*"
    ],
    "devDependencies": {
        "eslint": "^4.19.1",
        "eslint-config-synacor": "^3.0.5",
        "esm": "^3.2.25",
        "glob": "^8.0.3",
        "jest": "^29.3.1",
        "jest-preset-preact": "^4.0.5",
        "markdown": "^0.5.0",
        "per-env": "^1.0.2",
        "preact-cli": "^3.4.1",
        "preact-render-spy": "^1.2.1",
        "react-syntax-highlighter": "^15.5.0",
        "webpack-bundle-analyzer": "^4.7.0"
    },
    "dependencies": {
        "@preact/prerender-data-provider": "^3.0.1",
        "i": "^0.3.7",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-webpack-plugin": "^2.4.2",
        "markdown-to-jsx": "^7.1.7",
        "npm": "^9.1.2",
        "parse-md": "^2.0.5",
        "preact": "^10.11.3",
        "preact-cli-plugin-netlify": "^1.6.0",
        "preact-render-to-string": "^5.1.10",
        "preact-router": "^4.1.0",
        "sharp": "^0.31.1",
        "sitemap": "^7.1.1",
        "styled-components": "^5.3.6",
        "xml": "^1.0.1"
    },
    "jest": {
        "preset": "jest-preset-preact"
    },
    "bugs": {
        "url": "https://github.com/preactjs/preact-netlify/issues"
    },
    "homepage": "https://github.com/preactjs/preact-netlify#readme",
    "keywords": [
        "preact",
        "netlify"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/preactjs/preact-netlify.git"
    }
}
